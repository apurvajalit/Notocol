{
    "manifest_version": 2,
    "name": "Chimpu",
    "version": "0.1",
    "description": "Your Smart Study Assistant",
    "background": {
        
        "scripts": [ "event.js" ],
        "persistent": false
    },
    

    "browser_action": {
        "default_icon": "img/icon.png"
        //"default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ]
}
